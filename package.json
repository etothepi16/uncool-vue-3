{
  "name": "uncool",
  "version": "0.0.1",
  "main": "src/index.js",
  "description": "Vue.js plugin that allows page elements to be easily mutable using Firebase.",
  "repository": "https://github.com/etothepi16/uncool-vue-3",
  "license": "GPL-3.0",
  "keywords": [
    "uncool",
    "Vue",
    "Firebase",
    "CMS",
    "simple"
  ],
  "scripts": {
    "uncool": "npm version patch"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-useless-escape": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "dependencies": {
    "save": "^2.9.0",
    "vue3-editor": "^0.1.1"
  }
}
